<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.optimove.sdk.optimove_sdk">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:fullBackupContent="@xml/optimove_backup_rules"
        android:dataExtractionRules="@xml/optimove_backup_rules_api31"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <provider
            android:name=".undecided.KumulosInitProvider"
            android:authorities="${applicationId}.kumulosinitprovider"
            android:exported="false" />
        <activity
            android:name=".undecided.PushOpenInvisibleActivity"
            android:noHistory="true"
            android:excludeFromRecents="true"
            android:theme="@android:style/Theme.Translucent"
            android:exported="false" />
        <service
            android:name=".main.app_update_listener.AppUpdateService"
            android:exported="false"
            android:permission="android.permission.BIND_JOB_SERVICE" />
    </application>
</manifest>